{
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-object-assign",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ],
    [
      "import",
      {
      "libraryName": "antd",
      "libraryDirectory": "lib",
      "style": "css"
      }
    ]
  ],
  "presets": [
    "react",
    "stage-0",
    "es2015"
  ]
}
